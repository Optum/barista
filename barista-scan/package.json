{
  "name": "barista-scan",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rm -Rf dist && tsc -p ./tsconfig.build.json",
    "build:prod": "yarn build",
    "format": "prettier \"src/**/*.ts\"",
    "format:ci": "yarn format -c",
    "format:watch": "onchange '**/*.*' -- yarn format-lint",
    "format-lint": "yarn format --write  && yarn lint --fix",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:ci": "yarn lint",
    "start:static": "yarn build && ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:nobuild": "nodemon --config nodemon.json",
    "debug": "yarn start:debug",
    "start": "export NODE_ENV=development && yarn build && yarn start:nobuild",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon --ignore ./tools\" \"tsc -w -p ./tsconfig.build.json\" ",
    "start:prod": "node --max-old-space-size=8192 dist/main.js",
    "seed:create": "typeorm migration:create -c seed -n ",
    "seed:run": "typeorm migration:run -c seed",
    "seed:revert": "typeorm migration:revert -c seed",
    "db:seed": "yarn typeorm migration:run -c seed",
    "db:seed:test": "TEST_DB=true yarn typeorm migration:run -c seed",
    "db:seed:demo": "yarn typeorm migration:run -c demo-seed",
    "db:migrate": "yarn typeorm migration:run",
    "db:migrate:test": "TEST_DB=true yarn db:migrate",
    "db:reset:test": "yarn db:drop:test && yarn db:migrate:test && yarn db:seed:test",
    "db:sync": "yarn typeorm schema:sync",
    "db:drop": "yarn typeorm schema:drop",
    "db:drop:test": "TEST_DB=true yarn typeorm schema:drop",
    "db:reset": "yarn db:drop && yarn db:migrate && yarn db:seed",
    "test:once": "yarn build && jest",
    "test": "yarn test:once --watch",
    "test:ci": "yarn test:once --ci --reporters=default --reporters=jest-junit --coverage",
    "test:cov": "yarn test:once --coverage",
    "test:debug": "yarn build && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:resetdb": "yarn db:reset:test && yarn test:e2e",
    "test:e2e:once": "yarn build && TEST_DB=true jest --config ./test/jest-e2e.json",
    "test:e2e": "yarn test:e2e:once --watch",
    "test:clear": "jest --clearCache",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "link:barista": "cd ./src && ln -s ../../barista-api/src/models ./models && ln -s ../../barista-api/src/services ./services && ln -s ../../barista-api/src/shared ./shared && ln -s ../../barista-api/src/db ./db && cd ..",
    "copy:link:barista": "cd ./src && rm -Rf ./models && cp -Rf ../../barista-api/src/models ./ && rm -Rf ./services && cp -Rf ../../barista-api/src/services ./ && rm -Rf ./shared && cp -Rf ../../barista-api/src/shared ./ && rm -Rf ./db && cp -Rf ../../barista-api/src/db ./ && cd ..",
    "hads": "hads -o --port 4042",
    "majestic": "TEST_DB=true majestic --app",
    "majestic:e2e": "TEST_DB=true majestic --config=e2e --app",
    "write:app-info": "yarn write:commit-hash; yarn write:app-version; yarn write:app-environment; cat src/info/*",
    "write:commit-hash": "COMMIT_HASH=$(git log --pretty=format:%h -n 1); replace 'HEAD' $COMMIT_HASH src/info/commit-hash.ts",
    "write:app-version": "APP_VERSION=$(node -pe \"require('./package.json').version\"); replace '0.0.0.0' $APP_VERSION src/info/app-version.ts",
    "write:app-environment": "APP_ENVIRONMENT=${ENV:-undefined}; replace 'local' $APP_ENVIRONMENT src/info/app-environment.ts"
  },
  "dependencies": {
    "@nestjs/bull": "^0.1.1",
    "@nestjs/common": "^7.1.2",
    "@nestjs/core": "^7.1.2",
    "@nestjs/cqrs": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/microservices": "^7.1.2",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.1.2",
    "@nestjs/swagger": "^4.5.9",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-request": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "@types/axios": "^0.14.0",
    "@types/bull": "^3.12.2",
    "@types/cheerio": "^0.22.17",
    "@types/express-serve-static-core": "^4.17.0",
    "@types/find": "^0.2.1",
    "@types/html-to-text": "^1.4.31",
    "@types/lodash": "^4.14.149",
    "@types/nodegit": "^0.26.2",
    "@types/nodemailer": "^6.4.0",
    "@types/p-all": "^2.0.0",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.46",
    "@types/shelljs": "^0.8.7",
    "@types/spdx-correct": "^2.0.0",
    "@types/string-similarity": "^3.0.0",
    "@types/tmp": "^0.1.0",
    "@types/xml2js": "^0.4.5",
    "all-unpacker": "^0.1.3",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "bull": "^3.13.0",
    "cheerio": "^1.0.0-rc.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "convert-to-object": "^0.0.4",
    "core-js": "^3",
    "csv-parse": "^4.8.8",
    "dotenv": "^8.0.0",
    "elastic-apm-node": "^3.5.0",
    "execa": "^4.0.0",
    "find": "^0.3.0",
    "hads": "^2.0.0",
    "html-to-text": "^5.1.1",
    "lodash": "^4.17.15",
    "maven": "^4.7.0",
    "module-alias": "^2.2.2",
    "node-wget": "^0.4.3",
    "nodemailer": "^6.4.6",
    "nuget": "^2.0.2",
    "p-all": "^3.0.0",
    "p-iteration": "^1.1.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.2",
    "promised-ldap": "^0.3.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "shelljs": "^0.8.3",
    "simple-git": "^1.132.0",
    "spawn-rx": "^3.0.0",
    "spdx-correct": "^3.1.0",
    "spdx-expression-parse": "^3.0.0",
    "string-similarity": "^4.0.1",
    "tmp": "^0.1.0",
    "typeorm": "^0.2.24",
    "xml2js": "^0.4.23",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.8",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/supertest": "^2.0.8",
    "concurrently": "^5.1.0",
    "jest": "25.3.0",
    "jest-junit": "^10.0.0",
    "majestic": "^1.6.2",
    "nodemon": "^2.0.3",
    "onchange": "^6.1.0",
    "prettier": "^2.0.4",
    "replace": "^1.1.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.3.1",
    "ts-node": "8.8.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.1",
    "tslint-etc": "^1.10.1",
    "tslint-lines-between-class-members": "^1.3.6",
    "typescript": "3.8.3",
    "wait-on": "^4.0.2"
  },
  "_moduleAliases": {
    "@app": "dist"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@app/(.*)$": "<rootDir>/$1"
    },
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "suiteName": "Barista Scan Tests",
    "outputDirectory": ".",
    "outputName": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "majestic": {
    "jestScriptPath": "node_modules/jest/bin/jest.js",
    "configs": {
      "e2e": {
        "args": [
          "--config=./test/jest-e2e.json"
        ]
      }
    }
  }
}
